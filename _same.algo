ALGORITHM sameArrayVal

VAR 

    array1 : ARRAY_OF INTEGER[5];
    array2 : ARRAY_OF INTEGER[5];
    Sum = INTEGER := 0;

BEGIN 
    FOR (i = 0 ; i < array1.length() ; i++) DO 
        IF (array1[i] == array2[i]) THEN
            Sum := array1[i] + array2[i]
        END_IF
    END_FOR
    Write(Sum)
END
